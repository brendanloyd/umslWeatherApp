<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UmslWeatherApp">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="413dp"
        android:layout_height="288dp"
        android:layout_marginBottom="32dp"
        map:layout_constraintBottom_toTopOf="@+id/temp"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/temp"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginBottom="48dp"
        android:text="Temperature"
        android:textColor="#000"
        android:textSize="20sp"
        map:layout_constraintBottom_toTopOf="@+id/maxTemp"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.112"
        map:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/maxTemp"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginBottom="48dp"
        android:text="Description"
        android:textColor="#000"
        android:textSize="20sp"
        map:layout_constraintBottom_toTopOf="@+id/minTemp"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.112"
        map:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/minTemp"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginBottom="100dp"
        android:text="Humidity"
        android:textColor="#000"
        android:textSize="20sp"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.112"
        map:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>